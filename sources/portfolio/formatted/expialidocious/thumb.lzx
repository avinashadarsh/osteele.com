    </code></span><span class="element"><span class="markup">&lt;</span><span class="starttag" id="d0e10">method</span> <span class="attrname">event</span>="<span class="attrvalue">oninit</span>"<span class="markup">&gt;</span>
      drawContour(0.35,0.35);
      closePath();
      this.strokeStyle = 0xff2020;
      //this.strokeStyle = 0x2020ff;
      stroke();
      drawContour(0,0);
      this.fillStyle = 0xff8080;
      //this.fillStyle = 0x8080ff;
      fill();
    <span class="markup">&lt;/</span><span class="endtag">method</span><span class="markup">&gt;</span></span><span class="method-element-text"><code>
    </code></span><span class="element"><span class="markup">&lt;</span><span class="starttag" id="d0e13">method</span> <span class="attrname">name</span>="<span class="attrvalue"><span class="name">drawContour</span></span>" <span class="attrname">args</span>="<span class="attrvalue">dx,dy</span>"<span class="markup">&gt;</span>
      var tl=0, tr=0, bl=0, br=0;
      if (side == 'bottom') bl=br=1;
      if (side == 'left') tl=bl=1;
      if (side == 'right') tr=br=1;
      var rx = 10;
      var ry = 10;
      beginPath();
      moveTo(dx+tl*rx, dy); // [0,0]
      if (tl) quadraticCurveTo(dx, dy, dx, ry+dy);
      lineTo(dx, height-bl*ry+dy); // [0,1]
      if (bl) quadraticCurveTo(dx, height+dy, rx+dx, height+dy);
      lineTo(width-br*rx+dx, height+dy); //[1,1]
      if (br) quadraticCurveTo(width+dx, height+dy, width+dx, height-ry+dy);
      lineTo(width+dx, ry*tr+dy); //[1,0]
      if (tr) quadraticCurveTo(width+dx, 0+dy, width-rx+dx, 0+dy);
    <span class="markup">&lt;/</span><span class="endtag">method</span><span class="markup">&gt;</span></span>
